<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="library.infrastructure.datasource.bookonloan.BookOnLoanMapper">
    <select id="newBookOnLoanIdentifier" resultType="Integer">
        SELECT NEXTVAL('貸出ID')
    </select>

    <insert id="insertBookOnLoan">
        INSERT INTO 貸出(貸出ID, 会員番号, 蔵書コード, 貸出日)
        VALUES (#{bookOnLoanId}, #{memberNumber.value}, #{bookCollectionCode.value}, #{loanDate.value.value})
    </insert>
</mapper>